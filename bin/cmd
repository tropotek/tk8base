#!/usr/bin/env php
<?php
include(dirname(__DIR__) . '/_prepend.php');

try {
    set_time_limit(0);
    $app = $factory->getConsole();

    // Setup Local Console Commands
    $app->add(new \App\Console\Cron());
    if ($config->isDebug()) {
        //$app->add(new \App\Console\TestData());
        //$app->add(new \App\Console\Test());
    }

    $app->run();
} catch (Exception $e) {
    echo $e->__toString();
}
